package browserTest.pages;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class telAvivJobs {

	WebDriver driver;
	
	public telAvivJobs(WebDriver driver){
		this.driver = driver;
	}
	
	public void compareNumAndJubs(int number) {
		WebElement num;
		try {
			num = driver.findElement(By.xpath("Xpath=//*[contains(text(),'open positions')]"));
		}
		catch (Exception e) {
			System.out.print("Cant Find Objct - openposisiions on telAviv Jobs Page");
		}
		
		num.getAttribute("col-md-4");
		
	}
}
